{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CalendarEventDetail",
  "type": "object",
  "required": [
    "calendar_id",
    "summary",
    "start",
    "is_recurring",
    "meeting_code"
  ],
  "properties": {
    "calendar_id": {
      "type": "string"
    },
    "summary": {
      "type": "string"
    },
    "start": {
      "type": "string",
      "format": "date-time"
    },
    "is_recurring": {
      "type": "boolean"
    },
    "meeting_code": {
      "type": ["string", "null"]
    }
  },
  "additionalProperties": false
}
